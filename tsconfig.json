{
  "allowSyntheticDefaultImports": true,
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "esModuleInterop": true,
    "lib": ["es2015", "dom", "es6"],
    "baseUrl": ".",
    "paths": {
      "$redux/*": ["src/redux/*"],
      "$components/*": ["src/components/*"],
      "$constants/*": ["src/constants/*"],
      "$containers/*": ["src/containers/*"],
      "$styles/*": ["src/styles/*"],
      "$utils/*": ["src/utils/*"]
    },
  }
}